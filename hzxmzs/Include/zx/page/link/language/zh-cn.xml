		<listdescription>栏目简述</listdescription>
		<info>属性</info>
		<suplist>上级列表</suplist>
		<key>搜索关键字</key>
		<ktitle>标题</ktitle>
		<kdescription>内容</kdescription>
		<image>logo图片</image>
		<urlpath>URL地址</urlpath>
		
		<grade>
			<title>阅读权限</title>
			<g0>任何人都可以访问</g0>
			<g1>限站内会员访问</g1>
		</grade>

		<path>目录 (列表的目录)</path>
		<fpath>路径</fpath>
		<list>所属列表</list>
		<newlist>目标列表</newlist>
		<listtemplate1>列表页外部模板</listtemplate1>
		<listtemplate2>列表页内部模板</listtemplate2>
		<pagetemplate1>内容页外部模板</pagetemplate1>
		<pagetemplate2>内容页内部模板</pagetemplate2>
