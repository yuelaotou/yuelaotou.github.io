		<grade>所属会员组</grade>
		<grade0>开放</grade0>
		<grade1>会员</grade1>
	</list>
	
	<label>
		<title>标题</title>
		<listtitle>栏目标题</listtitle>
		<rn>每页显示</rn>
		<authornone>不详</authornone>
		<up>置顶</up>
		<show>显示</show>
		<head>头条</head>
