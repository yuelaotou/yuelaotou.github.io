	<list>
		<id>ID</id>
		<listname>列表名称</listname>
		<title>标题</title>
		<group>分组名称</group>
		<num>对应编号</num>
		<user>用户组</user>
		<alluser>所有会员</alluser>
		<manage>操作</manage>
		<count>项目数</count>
		<file>文档</file>
		<path>路径</path>
		<move>移动</move>
		<sublist>下属项目总数</sublist>
